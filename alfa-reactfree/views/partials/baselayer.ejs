// create the underlying layer with the actual map
var center = SMap.Coords.fromWGS84(14.4734721275829, 50.0698471052258); //default map center
var map = new SMap(JAK.gel("map"), center, 15); //this number is the zoom level - higher number means closer zoom
map.addDefaultLayer(SMap.DEF_BASE).enable();
map.addDefaultControls();

// create and enable a marker layer on the map
var layer = new SMap.Layer.Marker();
map.addLayer(layer);
layer.enable();
